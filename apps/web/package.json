{
  "name": "@hyperlocal/web",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "dev": "react-router dev --host",
    "make:replica": "tsx --env-file .env scripts/make-local-replica.ts",
    "upload:db": "tsx --env-file .env scripts/upload-db.ts",
    "build": "react-router build && tsx --env-file=.env scripts/build-sitemap.ts",
    "build:sitemap": "tsx --env-file=.env scripts/build-sitemap.ts",
    "start": "node ./build/server/index.js",
    "lint": "biome lint .",
    "format": "prettier --check . --experimental-cli",
    "format:fix": "prettier --write . --experimental-cli",
    "typecheck": "react-router typegen && tsc",
    "test": "vitest --run",
    "validate": "run-s lint format typecheck test"
  },
  "dependencies": {
    "@coji/zodix": "catalog:",
    "@date-fns/tz": "catalog:",
    "@date-fns/utc": "catalog:",
    "@hono/node-server": "catalog:",
    "@hyperlocal/consts": "workspace:*",
    "@hyperlocal/db": "workspace:*",
    "@hyperlocal/google-place-api": "workspace:*",
    "@libsql/client": "catalog:",
    "@libsql/kysely-libsql": "catalog:",
    "@react-router/node": "catalog:",
    "@react-router/remix-routes-option-adapter": "catalog:",
    "class-variance-authority": "catalog:",
    "clsx": "catalog:",
    "date-fns": "catalog:",
    "dotenv": "catalog:",
    "embla-carousel-react": "catalog:",
    "hono": "catalog:",
    "isbot": "catalog:",
    "kysely": "catalog:",
    "lucide-react": "catalog:",
    "mdx-bundler": "catalog:",
    "minio": "catalog:",
    "next-themes": "catalog:",
    "nprogress": "catalog:",
    "radix-ui": "catalog:",
    "react": "catalog:",
    "react-dom": "catalog:",
    "react-router": "catalog:",
    "react-router-hono-server": "catalog:",
    "react-twc": "catalog:",
    "remix-flat-routes": "catalog:",
    "remix-utils": "catalog:",
    "tailwind-merge": "catalog:",
    "ts-pattern": "catalog:",
    "zod": "catalog:"
  },
  "devDependencies": {
    "@biomejs/biome": "catalog:",
    "@react-router/dev": "catalog:",
    "@tailwindcss/vite": "catalog:",
    "@types/node": "catalog:",
    "@types/nprogress": "catalog:",
    "@types/react": "catalog:",
    "@types/react-dom": "catalog:",
    "autoprefixer": "catalog:",
    "concurrently": "catalog:",
    "dotenv-cli": "catalog:",
    "npm-run-all": "catalog:",
    "prettier": "catalog:",
    "prettier-plugin-organize-imports": "catalog:",
    "prettier-plugin-tailwindcss": "catalog:",
    "prisma": "catalog:",
    "prisma-kysely": "catalog:",
    "tailwindcss": "catalog:",
    "tailwindcss-animate": "catalog:",
    "tsx": "catalog:",
    "typescript": "catalog:",
    "vite": "catalog:",
    "vite-tsconfig-paths": "catalog:",
    "vitest": "catalog:"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
